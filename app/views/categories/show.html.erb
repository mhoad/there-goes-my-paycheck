<!-- Start contain -->
  <section id="contain" itemprop="mainContentOfPage">
    <div class="container">
      <div class="row">
      <div class="span3">
        <div class="navaside">
          <h5>Product Categories</h5>
          <ul class="nav">
            <li class="active">
              <a href="#"><%= @category.name %></a></li>
            <% @category.subcategories.each do |category| %>
              <li><%= link_to category.name, category_path(category) %></li>
            <% end %>
            <% @top_level_categories.each do |category| %>
              <li><%= link_to category.name, category_path(category) %></li>
            <% end %>
          </ul>
        </div>
      </div>

      <div class="span9">
        
        <div class="marginbot35">

          <h3><%= @category.name %></h3>
          <ul itemprop="breadcrumb" class="breadcrumb">
            <li><span class="glyphicon glyphicon-home"></span> <%= link_to "Home", root_path %></li> &gt;
            <% if !@category.parent_category.nil? %>              
              <li><a href="#"><%= link_to @category.parent_category.name, category_path(@category.parent_category) %></a> </li> &gt;
            <% end %>
            <li class="active"><%= @category.name %></li>
          </ul>
          <%= @category.description %>
        </div>      
        <div id="filter-bar" class="btn-group">
          <a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
          Sort item by
          <span class="caret"></span>
          </a>        
          <ul id="filters" class="dropdown-menu">
            <li><a href="#" data-filter=".latest">Latest items</a></li>
            <li><a href="#" data-filter=".popular">Popular items</a></li>
            <li><a href="#" data-filter=".premium">Premium items</a></li>
            <li><a href="#" data-filter=".free">Free items</a></li>
          </ul>
